%h2= t('Editing <%= singular_name %>')
= render :partial => 'form', :locals => {:<%= singular_name %> => @<%= singular_name %>}
%ul
  %li= link_to t('Show'), @<%= singular_name %>
  %li= link_to t('Back'), <%= plural_name %>_path
  %li= link_to t('Delete'), @<%= singular_name %>, :confirm => t('Are you sure'), :method => :delete
